@model Clothing_boutique_web.Models.Account;
@{
ViewData["Title"] = "Profile";
Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Information's Account</h3>
    </div><!-- /.box-header -->
    <!-- form start -->
    <form role="form" method="post" asp-area="admin" asp-controller="login" asp-action="profile">
        <div class="box-body">
        <div class="form-group">
            <label for="exampleInputEmail1">User Name</label>
            <input type="text" class="form-control" asp-for="UserName" value="@Model.UserName">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" asp-for="Password" value="@Model.Password">
        </div>
         <div class="form-group">
            <label for="exampleInputEmail1">Full Name</label>
            <input type="text" class="form-control" asp-for="FullName" value="@Model.FullName">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail">Email</label>
            <input type="email" class="form-control" asp-for="Email" value="@Model.Email">
        </div>
         <div class="form-group">
            <label for="exampleInputEmail1">Address</label>
            <input type="text" class="form-control" asp-for="Addresss" value="@Model.Addresss">
        </div>
        </div><!-- /.box-body -->

        <div class="box-footer">
        <button type="submit" class="btn btn-primary">Submit</button>
        <input type="hidden" class="form-control" asp-for="Id" value="@Model.Id">
        </div>
    </form>
</div>